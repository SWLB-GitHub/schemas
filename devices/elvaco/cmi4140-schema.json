{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://raw.githubusercontent.com/SWLB-GitHub/schemas/main/devices/elvaco/cmi4140-schema.json",
    "title": "Elvaco CMi4140 LoRaWAN 0x15",
    "description": "decoded Elvaco CMi4140 payload fields (standard frametype 0x15)",
    "type": "object",
	"properties": {
		"error": {
			"description": "transmission information from the sending device",
			"$ref": "https://raw.githubusercontent.com/SWLB-GitHub/schemas/main/misc/error-schema.json"
		},
		"alarm": {
			"description": "transmission information from the sending device",
			"$ref": "https://raw.githubusercontent.com/SWLB-GitHub/schemas/main/misc/alarm-schema.json"
		},
		"totalEnergy": {
			"description": "meter reading of the total energy used",
			"$ref": "https://raw.githubusercontent.com/SWLB-GitHub/schemas/main/misc/measurement-schema.json"
		},
		"totalVolume": {
			"description": "meter reading of the total volume of fluid",
			"$ref": "https://raw.githubusercontent.com/SWLB-GitHub/schemas/main/misc/measurement-schema.json"
		},
		"currentPower": {
			"description": "current power consumption",
			"$ref": "https://raw.githubusercontent.com/SWLB-GitHub/schemas/main/misc/measurement-schema.json"
		},
		"currentFlow": {
			"description": "current flowrate",
			"$ref": "https://raw.githubusercontent.com/SWLB-GitHub/schemas/main/misc/measurement-schema.json"
		},
		"tempFw": {
			"description": "forward temperature",
			"$ref": "https://raw.githubusercontent.com/SWLB-GitHub/schemas/main/misc/measurement-schema.json"
		},
		"tempRt": {
			"description": "return temperature",
			"$ref": "https://raw.githubusercontent.com/SWLB-GitHub/schemas/main/misc/measurement-schema.json"
		},
		"subdeviceSerialnumber": {
			"description": "serial number of connected Kamstrup Multical device",
			"$ref": "https://raw.githubusercontent.com/SWLB-GitHub/schemas/main/misc/attribute-schema.json"
		}
	}
}